{
  "private": true,
  "sideEffects": false,
  "scripts": {
    "postinstall": "prisma generate",
    "build": "remix build",
    "dev": "remix dev",
    "format": "prettier --write .",
    "lint": "eslint .",
    "setup": "prisma migrate dev",
    "start": "remix-serve build",
    "test:e2e:dev": "playwright test",
    "pretest:e2e:run": "npm run build",
    "test:e2e:run": "cross-env CI=true playwright test",
    "test:e2e:install": "npx playwright install chromium --with-deps",
    "typecheck": "tsc",
    "validate": "run-p lint typecheck"
  },
  "prettier": {},
  "eslintIgnore": [
    "/node_modules",
    "/build",
    "/public/build",
    "/playwright-report"
  ],
  "dependencies": {
    "@faker-js/faker": "^7.6.0",
    "@prisma/client": "^4.8.0",
    "@remix-run/node": "1.10.0-pre.5",
    "@remix-run/react": "1.10.0-pre.5",
    "@remix-run/serve": "1.10.0-pre.5",
    "@remix-run/server-runtime": "1.10.0-pre.5",
    "bcryptjs": "^2.4.3",
    "cuid": "^2.1.8",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "tiny-invariant": "^1.3.1"
  },
  "devDependencies": {
    "@playwright/test": "^1.29.1",
    "@remix-run/dev": "1.10.0-pre.5",
    "@remix-run/eslint-config": "1.10.0-pre.5",
    "@types/bcryptjs": "^2.4.2",
    "@types/eslint": "^8.4.10",
    "@types/node": "^18.11.18",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.10",
    "autoprefixer": "^10.4.13",
    "cross-env": "^7.0.3",
    "eslint": "^8.30.0",
    "eslint-config-prettier": "^8.5.0",
    "npm-run-all": "^4.1.5",
    "prettier": "2.8.1",
    "prisma": "^4.8.0",
    "tsx": "^3.12.1",
    "typescript": "^4.9.4"
  },
  "engines": {
    "node": ">=14"
  },
  "prisma": {
    "seed": "tsx --require tsconfig-paths/register prisma/seed.ts"
  }
}
